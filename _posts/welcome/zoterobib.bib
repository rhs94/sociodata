}

@article{yun_satisfaction_2019,
	title = {The {Satisfaction} with {Life} {Scale} and the {Subjective} {Well}-{Being} {Inventory} in the {General} {Korean} {Population}: {Psychometric} {Properties} and {Normative} {Data}},
	volume = {16},
	issn = {1660-4601},
